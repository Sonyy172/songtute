/* ============ Search ============ */
#search.fade {
  height: 100vh;
  width: 100vw;
  transition: opacity 0.5s;
}
#search {
  align-items: center;
  background: rgba(255, 255, 255, 0.95);
  height: 0;
  display: flex;
  justify-content: center;
  opacity: 0;
  position: fixed;
  transition: all 0.5s;
  width: 100vw;
  will-change: transform, opacity;
  z-index: -1;
}
#search:target {
  height: 100vh !important;
  opacity: 1;
  width: 100vw !important;
  z-index: 1040;
}
#search:target .close-btn {
  display: block;
}
#search:target .close-btn:hover {
  text-decoration: none;
}
#searchbox {
  background: transparent;
  border: solid rgba(0,0,0,0.5);
  border-width: 0 0 1px 0;
  color: #000;
  flex: 1 0 auto;
  font-size: 2rem;
  height: 2rem;
  max-width: 50%;
  outline: 0;
  padding: 2rem 1rem;
}
#searchbox::placeholder {
  color: rgba(0,0,0,0.7);
}
#search button {
  border: 0;
  background: none;
  font-size: 24px;
  color: rgba(0,0,0, 0.5);
  transition: .2s;
}
#search button:hover, #search button:focus, .close-btn:hover {
  cursor: pointer;
  color: rgba(0,0,0,1);
}
.close-btn {
  display: none;
  color: rgba(0,0,0,0.5);
  font-size: 2rem;
  position: absolute;
  top: 0.5rem;
  right: 2rem;
}
