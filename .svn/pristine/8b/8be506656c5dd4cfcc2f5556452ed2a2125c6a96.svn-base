.navbar-tute {
	background-color: #000;
  transition: all ease-in-out .1s;
  width: 100%;
  padding: 0;
}
.navbar-tute .nav-link {
	color: rgba(255,255,255,0.7);
	text-transform: uppercase;
	font-size: 1rem;
	line-height: 60px;
	margin: 0;
	padding: 0 15px !important;
	transition: all ease-in-out .3s;
}
.navbar-tute .active .nav-link {
	color: rgba(255,255,255,1);
}
.navbar-tute .dropdown-toggle::after {
	position: absolute;
	left: 50%;
	transform: translateX(-50%);
	bottom: 10px;
}
.navbar-tute .dropdown-toggle:hover::after {
	bottom: 5px;
}
.navbar-tute .active .dropdown-toggle, .navbar-tute .dropdown-toggle:hover {
	background-color: #fff;
	color: rgba(0,0,0,1);
}
.navbar-tute .dropdown-toggle:hover {
