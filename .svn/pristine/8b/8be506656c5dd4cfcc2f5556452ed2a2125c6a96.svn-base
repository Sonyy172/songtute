}
.navbar-tute .dropdown-menu {
	border: none;
	padding: 15px;
	margin: 0;
	-webkit-box-shadow: 0px 8px 17px 0px rgba(0,0,0,0.1);
	-moz-box-shadow: 0px 8px 17px 0px rgba(0,0,0,0.1);
	box-shadow: 0px 8px 17px 0px rgba(0,0,0,0.1);
	border-radius: 0;
	right: auto;
	left: 0;
}
.navbar-tute .dropdown-item {
	padding: 10px 20px 10px 30px;
	position: relative;
}
.navbar-tute .dropdown-item:before {
	content: '';
	width: 10px;
	height: 10px;
	background-image: url(../img/hex.svg);
	background-size: 10px 10px;
